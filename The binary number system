#include<stdio.h>
int main()
{
    int i,j,rows=12;
    for(int i=1;i<=rows; ++i)
    {
        if(i%2==0)
        {
            for(j=1;j<=i;++j)
            {
                if(j%2==0)
                {
                    printf("1");
                }
                else
                {
                    printf("0");
                }
            }
            printf("\n");
        }
    }
    return 0;
}
